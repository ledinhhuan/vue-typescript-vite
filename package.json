{
  "name": "vue-typescript-vite",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "analyzer": "vue-tsc --noEmit && vite build --mode analyzer",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "dayjs": "^1.11.1",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "prettier": "^2.5.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.34.7"
  },
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ledinhhuan/vue-typescript-vite.git"
  },
  "author": "dinhhuan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ledinhhuan/vue-typescript-vite/issues"
  },
  "homepage": "https://github.com/ledinhhuan/vue-typescript-vite#readme"
}
